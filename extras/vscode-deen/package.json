{
	"name": "deen",
	"displayName": "deen",
	"description": "Generic data decoding and encoding",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:deen.helloWorld",
		"onCommand:deen.base32Encode",
		"onCommand:deen.base32Decode",
		"onCommand:deen.base64Encode",
		"onCommand:deen.base64Decode",
		"onCommand:deen.base85Encode",
		"onCommand:deen.base85Decode",
		"onCommand:deen.urlEncode",
		"onCommand:deen.urlDecode",
		"onCommand:deen.htmlEncode",
		"onCommand:deen.htmlDecode",
		"onCommand:deen.jsonFormat",
		"onCommand:deen.jsonUnformat"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "deen.base32Encode",
				"title": "deen: Base32 Encode"
			},
			{
				"command": "deen.base32Decode",
				"title": "deen: Base32 Decode"
			},
			{
				"command": "deen.base64Encode",
				"title": "deen: Base64 Encode"
			},
			{
				"command": "deen.base64Decode",
				"title": "deen: Base64 Decode"
			},
			{
				"command": "deen.base85Encode",
				"title": "deen: Base85 Encode"
			},
			{
				"command": "deen.base85Decode",
				"title": "deen: Base85 Decode"
			},
			{
				"command": "deen.urlEncode",
				"title": "deen: URL Encode"
			},
			{
				"command": "deen.urlDecode",
				"title": "deen: URL Decode"
			},
			{
				"command": "deen.htmlEncode",
				"title": "deen: HTML Encode"
			},
			{
				"command": "deen.htmlDecode",
				"title": "deen: HTML Decode"
			},
			{
				"command": "deen.jsonFormat",
				"title": "deen: JSON Format"
			},
			{
				"command": "deen.jsonUnformat",
				"title": "deen: JSON Unformat"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
